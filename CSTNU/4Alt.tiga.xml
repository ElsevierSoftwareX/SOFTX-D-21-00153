<!DOCTYPE nta PUBLIC "-//Uppaal Team//DTD Flat System 1.1//EN"
                     "http://www.it.uu.se/research/group/darts/uppaal/flat-1_1.dtd">
<nta>
    <template>
        <name>g4AlternativeWFpaths_cstnu</name>
        <!--Clock and proposition declarations-->
        <declaration>clock tG, tDelta, tn2, tn6, tn17, tn7, tn10, tn16, tn0, tn14, tn8, tn4, tn12, tn13, tn3, tn15, tn5, tn9, tB_, tA_;
bool xn2, xn6, xn17, xn7, xn10, xn16, xn0, xn14, xn8, xn4, xn12, xn13, xn3, xn15, xn5, xn9, xB_, xA_;
int [-1,1] B = 0, A = 0;</declaration>
        <!--Node declarations-->
        <location id="agnes">
            <name>agnes</name>
            <urgent/>
        </location>
        <location id="vera">
            <name>vera</name>
        </location>
        <location id="goal">
            <name>goal</name>
        </location>
        <location id="go">
            <name>go</name>
            <urgent/>
        </location>
        <location id="go1">
            <name>go1</name>
            <urgent/>
        </location>
        <location id="go2">
            <name>go2</name>
            <urgent/>
        </location>
        <location id="go3">
            <name>go3</name>
            <urgent/>
        </location>
        <location id="go4">
            <name>go4</name>
            <urgent/>
        </location>
        <location id="go5">
            <name>go5</name>
            <urgent/>
        </location>
        <init ref="agnes"/>
        <!--Transition declarations-->
        <!--GAIN transition-->
        <transition action="gain" controllable="false">
            <source ref="vera"/>
            <target ref="agnes"/>
            <label kind="guard">tDelta &gt; 0</label>
            <label kind="assignment"/>
        </transition>
        <!--PASS transition-->
        <transition action="pass" controllable="false">
            <source ref="agnes"/>
            <target ref="vera"/>
            <label kind="guard"/>
            <label kind="assignment">tDelta := 0</label>
        </transition>
        <!--Transitions for clock setting-->
        <transition action="set_xn2" controllable="false">
            <source ref="agnes"/>
            <target ref="agnes"/>
            <label kind="guard">!xn2</label>
            <label kind="assignment">xn2 := true,tn2 := 0</label>
        </transition>
        <transition action="set_xn6" controllable="false">
            <source ref="agnes"/>
            <target ref="agnes"/>
            <label kind="guard">!xn6</label>
            <label kind="assignment">xn6 := true,tn6 := 0</label>
        </transition>
        <transition action="set_xn17" controllable="false">
            <source ref="agnes"/>
            <target ref="agnes"/>
            <label kind="guard">!xn17</label>
            <label kind="assignment">xn17 := true,tn17 := 0</label>
        </transition>
        <transition action="set_xn7" controllable="false">
            <source ref="agnes"/>
            <target ref="agnes"/>
            <label kind="guard">!xn7</label>
            <label kind="assignment">xn7 := true,tn7 := 0</label>
        </transition>
        <transition action="set_xn10" controllable="false">
            <source ref="agnes"/>
            <target ref="agnes"/>
            <label kind="guard">!xn10</label>
            <label kind="assignment">xn10 := true,tn10 := 0</label>
        </transition>
        <transition action="set_xn16" controllable="false">
            <source ref="agnes"/>
            <target ref="agnes"/>
            <label kind="guard">!xn16</label>
            <label kind="assignment">xn16 := true,tn16 := 0</label>
        </transition>
        <transition action="set_xn0" controllable="false">
            <source ref="agnes"/>
            <target ref="agnes"/>
            <label kind="guard">!xn0</label>
            <label kind="assignment">xn0 := true,tn0 := 0</label>
        </transition>
        <transition action="set_xn14" controllable="false">
            <source ref="agnes"/>
            <target ref="agnes"/>
            <label kind="guard">!xn14</label>
            <label kind="assignment">xn14 := true,tn14 := 0</label>
        </transition>
        <transition action="set_xn8" controllable="false">
            <source ref="agnes"/>
            <target ref="agnes"/>
            <label kind="guard">!xn8</label>
            <label kind="assignment">xn8 := true,tn8 := 0</label>
        </transition>
        <transition action="set_xn4" controllable="false">
            <source ref="agnes"/>
            <target ref="agnes"/>
            <label kind="guard">!xn4</label>
            <label kind="assignment">xn4 := true,tn4 := 0</label>
        </transition>
        <transition action="set_xn12" controllable="false">
            <source ref="agnes"/>
            <target ref="agnes"/>
            <label kind="guard">!xn12</label>
            <label kind="assignment">xn12 := true,tn12 := 0</label>
        </transition>
        <!--Transitions for proposition setting-->
        <transition action="set_B" controllable="false">
            <source ref="agnes"/>
            <target ref="agnes"/>
            <label kind="guard">!xB_ &amp;&amp; B == 0</label>
            <label kind="assignment">xB_ := true, tB_ := 0</label>
        </transition>
        <transition action="set_B_false" controllable="true">
            <source ref="vera"/>
            <target ref="vera"/>
            <label kind="guard">xB_ &amp;&amp; B == 0</label>
            <label kind="assignment">B := -1, tDelta := 0</label>
        </transition>
        <transition action="set_B_true" controllable="true">
            <source ref="vera"/>
            <target ref="vera"/>
            <label kind="guard">xB_ &amp;&amp; B == 0</label>
            <label kind="assignment">B := 1, tDelta := 0</label>
        </transition>
        <transition action="Bnot_set" controllable="false">
            <source ref="agnes"/>
            <target ref="goal"/>
            <label kind="guard">xB_ &amp;&amp; B == 0 &amp;&amp; tDelta &gt; 0</label>
            <label kind="assignment"/>
        </transition>
        <transition action="set_A" controllable="false">
            <source ref="agnes"/>
            <target ref="agnes"/>
            <label kind="guard">!xA_ &amp;&amp; A == 0</label>
            <label kind="assignment">xA_ := true, tA_ := 0</label>
        </transition>
        <transition action="set_A_false" controllable="true">
            <source ref="vera"/>
            <target ref="vera"/>
            <label kind="guard">xA_ &amp;&amp; A == 0</label>
            <label kind="assignment">A := -1, tDelta := 0</label>
        </transition>
        <transition action="set_A_true" controllable="true">
            <source ref="vera"/>
            <target ref="vera"/>
            <label kind="guard">xA_ &amp;&amp; A == 0</label>
            <label kind="assignment">A := 1, tDelta := 0</label>
        </transition>
        <transition action="Anot_set" controllable="false">
            <source ref="agnes"/>
            <target ref="goal"/>
            <label kind="guard">xA_ &amp;&amp; A == 0 &amp;&amp; tDelta &gt; 0</label>
            <label kind="assignment"/>
        </transition>
        <!--Transitions for contingent-constraint setting-->
        <transition action="set_xn13" controllable="true">
            <source ref="vera"/>
            <target ref="vera"/>
            <label kind="guard">xn12 &amp;&amp; !xn13 &amp;&amp; (tn12 &gt;= 3) &amp;&amp; (tn12 &lt;= 8)</label>
            <label kind="assignment">xn13 := true, tn13 := 0, tDelta := 0</label>
        </transition>
        <transition action="cv_tn13" controllable="false">
            <source ref="agnes"/>
            <target ref="goal"/>
            <label kind="guard">xn12 &amp;&amp; !xn13 &amp;&amp; (tn12 &gt; 8)</label>
            <label kind="assignment"/>
        </transition>
        <transition action="set_xn5" controllable="true">
            <source ref="vera"/>
            <target ref="vera"/>
            <label kind="guard">xn4 &amp;&amp; !xn5 &amp;&amp; (tn4 &gt;= 4) &amp;&amp; (tn4 &lt;= 6)</label>
            <label kind="assignment">xn5 := true, tn5 := 0, tDelta := 0</label>
        </transition>
        <transition action="cv_tn5" controllable="false">
            <source ref="agnes"/>
            <target ref="goal"/>
            <label kind="guard">xn4 &amp;&amp; !xn5 &amp;&amp; (tn4 &gt; 6)</label>
            <label kind="assignment"/>
        </transition>
        <transition action="set_xn15" controllable="true">
            <source ref="vera"/>
            <target ref="vera"/>
            <label kind="guard">xn14 &amp;&amp; !xn15 &amp;&amp; (tn14 &gt;= 5) &amp;&amp; (tn14 &lt;= 7)</label>
            <label kind="assignment">xn15 := true, tn15 := 0, tDelta := 0</label>
        </transition>
        <transition action="cv_tn15" controllable="false">
            <source ref="agnes"/>
            <target ref="goal"/>
            <label kind="guard">xn14 &amp;&amp; !xn15 &amp;&amp; (tn14 &gt; 7)</label>
            <label kind="assignment"/>
        </transition>
        <transition action="set_xn9" controllable="true">
            <source ref="vera"/>
            <target ref="vera"/>
            <label kind="guard">xn8 &amp;&amp; !xn9 &amp;&amp; (tn8 &gt;= 2) &amp;&amp; (tn8 &lt;= 7)</label>
            <label kind="assignment">xn9 := true, tn9 := 0, tDelta := 0</label>
        </transition>
        <transition action="cv_tn9" controllable="false">
            <source ref="agnes"/>
            <target ref="goal"/>
            <label kind="guard">xn8 &amp;&amp; !xn9 &amp;&amp; (tn8 &gt; 7)</label>
            <label kind="assignment"/>
        </transition>
        <transition action="set_xn3" controllable="true">
            <source ref="vera"/>
            <target ref="vera"/>
            <label kind="guard">xn2 &amp;&amp; !xn3 &amp;&amp; (tn2 &gt;= 2) &amp;&amp; (tn2 &lt;= 6)</label>
            <label kind="assignment">xn3 := true, tn3 := 0, tDelta := 0</label>
        </transition>
        <transition action="cv_tn3" controllable="false">
            <source ref="agnes"/>
            <target ref="goal"/>
            <label kind="guard">xn2 &amp;&amp; !xn3 &amp;&amp; (tn2 &gt; 6)</label>
            <label kind="assignment"/>
        </transition>
        <!--WIN transitions-->
        <transition action="win_labelled1A" controllable="true">
            <source ref="go"/>
            <target ref="go1"/>
            <label kind="guard">(A == 1)</label>
            <label kind="assignment"/>
        </transition>
        <transition action="win_labelled1" controllable="true">
            <source ref="go"/>
            <target ref="go1"/>
            <label kind="guard">(tA_ - tn5 &lt;= 0) &amp;&amp; (tA_ - tn4 &lt;= -1) &amp;&amp; (tn4 - tA_ &lt;= 1) &amp;&amp; (tn6 - tn5 &lt;= 1) &amp;&amp; (tn5 - tn6 &lt;= -1)</label>
            <label kind="assignment"/>
        </transition>
        <transition action="win_labelled2A" controllable="true">
            <source ref="go1"/>
            <target ref="go2"/>
            <label kind="guard">(A == 1)</label>
            <label kind="assignment"/>
        </transition>
        <transition action="win_labelled2B" controllable="true">
            <source ref="go1"/>
            <target ref="go2"/>
            <label kind="guard">(B == -1)</label>
            <label kind="assignment"/>
        </transition>
        <transition action="win_labelled2" controllable="true">
            <source ref="go1"/>
            <target ref="go2"/>
            <label kind="guard">(tn13 - tn4 &lt;= 27) &amp;&amp; (tn4 - tn13 &lt;= -17)</label>
            <label kind="assignment"/>
        </transition>
        <transition action="win_labelled3A" controllable="true">
            <source ref="go2"/>
            <target ref="go3"/>
            <label kind="guard">(A == -1)</label>
            <label kind="assignment"/>
        </transition>
        <transition action="win_labelled3" controllable="true">
            <source ref="go2"/>
            <target ref="go3"/>
            <label kind="guard">(tn2 - tA_ &lt;= 2) &amp;&amp; (tn6 - tn3 &lt;= 1) &amp;&amp; (tn3 - tn6 &lt;= -1) &amp;&amp; (tA_ - tn3 &lt;= 0) &amp;&amp; (tA_ - tn2 &lt;= -2)</label>
            <label kind="assignment"/>
        </transition>
        <transition action="win_labelled4A" controllable="true">
            <source ref="go3"/>
            <target ref="go4"/>
            <label kind="guard">(A == -1)</label>
            <label kind="assignment"/>
        </transition>
        <transition action="win_labelled4B" controllable="true">
            <source ref="go3"/>
            <target ref="go4"/>
            <label kind="guard">(B == -1)</label>
            <label kind="assignment"/>
        </transition>
        <transition action="win_labelled4" controllable="true">
            <source ref="go3"/>
            <target ref="go4"/>
            <label kind="guard">(tn13 - tn2 &lt;= 27) &amp;&amp; (tn2 - tn13 &lt;= -22)</label>
            <label kind="assignment"/>
        </transition>
        <transition action="win_labelled5B" controllable="true">
            <source ref="go4"/>
            <target ref="go5"/>
            <label kind="guard">(B == 1)</label>
            <label kind="assignment"/>
        </transition>
        <transition action="win_labelled5" controllable="true">
            <source ref="go4"/>
            <target ref="go5"/>
            <label kind="guard">(tB_ - tn15 &lt;= 0) &amp;&amp; (tn16 - tn15 &lt;= 1) &amp;&amp; (tn15 - tn16 &lt;= -1) &amp;&amp; (tn14 - tB_ &lt;= 1) &amp;&amp; (tB_ - tn14 &lt;= -1)</label>
            <label kind="assignment"/>
        </transition>
        <transition action="win_labelled6B" controllable="true">
            <source ref="go5"/>
            <target ref="goal"/>
            <label kind="guard">(B == -1)</label>
            <label kind="assignment"/>
        </transition>
        <transition action="win_labelled6" controllable="true">
            <source ref="go5"/>
            <target ref="goal"/>
            <label kind="guard">(tB_ - tn13 &lt;= 0) &amp;&amp; (tB_ - tn12 &lt;= -1) &amp;&amp; (tn16 - tn13 &lt;= 1) &amp;&amp; (tn12 - tB_ &lt;= 1) &amp;&amp; (tn13 - tn16 &lt;= -1)</label>
            <label kind="assignment"/>
        </transition>
        <transition action="win_unlabelled" controllable="false">
            <source ref="agnes"/>
            <target ref="go"/>
            <label kind="guard">xn2 &amp;&amp; xn6 &amp;&amp; xn17 &amp;&amp; xn7 &amp;&amp; xn10 &amp;&amp; xn16 &amp;&amp; xn0 &amp;&amp; xn14 &amp;&amp; xn8 &amp;&amp; xn4 &amp;&amp; xn12 &amp;&amp; xn13 &amp;&amp; xn3 &amp;&amp; xn15 &amp;&amp; xn5 &amp;&amp; xn9 &amp;&amp; xB_ &amp;&amp; xA_ &amp;&amp; (tn0 - tn17 &lt;= -15) &amp;&amp; (tn17 - tn16 &lt;= 2) &amp;&amp; (tn6 - tn7 &lt;= -1) &amp;&amp; (tn7 - tn6 &lt;= 2) &amp;&amp; (tB_ - tn10 &lt;= 10) &amp;&amp; (tn9 - tn10 &lt;= -1) &amp;&amp; (tn16 - tn17 &lt;= -1) &amp;&amp; (tn7 - tn8 &lt;= -1) &amp;&amp; (tn10 - tn9 &lt;= 1) &amp;&amp; (tn8 - tn7 &lt;= 1) &amp;&amp; (tn0 - tA_ &lt;= -1) &amp;&amp; (tn10 - tB_ &lt;= -1) &amp;&amp; (tn17 - tn0 &lt;= 34) &amp;&amp; (tA_ - tn0 &lt;= 5)</label>
            <label kind="assignment"/>
        </transition>
        <system>_processMain = g4AlternativeWFpaths_cstnu();
		system _processMain;</system>
    </template>
